{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://blog-app-six-khaki.vercel.app/api/$1"
      }
    ],
    "builds": [
      {
        "src": "src/index.jsx",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  